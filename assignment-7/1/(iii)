#include <iostream>
using namespace std;

void bubbleSort(int a[], int n) {
    for (int i = 0; i < n - 1; i++)
        for (int j = 0; j < n - i - 1; j++)
            if (a[j] > a[j + 1])
                swap(a[j], a[j + 1]);
}

int main() {
    int a[5] = {29, 10, 14, 37, 13};
    bubbleSort(a, 5);
    for (int i = 0; i < 5; i++) cout << a[i] << " ";
}
