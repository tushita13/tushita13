#include <bits/stdc++.h>
using namespace std;

int main() {
    int V = 5;
    vector<vector<int>> adj(V);
    adj[0] = {1,2};
    adj[1] = {0,3};
    adj[2] = {0,4};
    adj[3] = {1};
    adj[4] = {2};

    vector<bool> vis(V,false);
    queue<int> q;
    q.push(0);
    vis[0] = true;

    while(!q.empty()) {
        int n = q.front(); q.pop();
        cout << n << " ";
        for(int x : adj[n])
            if(!vis[x]) vis[x] = true, q.push(x);
    }
}
