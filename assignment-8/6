#include <bits/stdc++.h>
using namespace std;

class PriorityQueue {
    vector<int> h;

    void heapifyUp(int i){
        while(i>0){
            int p=(i-1)/2;
            if(h[p] < h[i]) swap(h[p],h[i]);
            else break;
            i=p;
        }
    }

    void heapifyDown(int i){
        int n=h.size();
        while(true){
            int l=2*i+1, r=2*i+2, m=i;
            if(l<n && h[l]>h[m]) m=l;
            if(r<n && h[r]>h[m]) m=r;
            if(m!=i){
                swap(h[i],h[m]);
                i=m;
            } else break;
        }
    }

public:
    void push(int x){
        h.push_back(x);
        heapifyUp(h.size()-1);
    }

    void pop(){
        if(h.empty()) return;
        h[0]=h.back();
        h.pop_back();
        heapifyDown(0);
    }

    int top(){
        if(h.empty()) return -1;
        return h[0];
    }

    bool empty(){
        return h.empty();
    }

    int size(){
        return h.size();
    }
};

int main(){
    PriorityQueue pq;
    pq.push(30);
    pq.push(10);
    pq.push(50);
    pq.push(40);

    cout<<pq.top()<<endl;

    pq.pop();
    cout<<pq.top()<<endl;

    pq.pop();
    cout<<pq.top()<<endl;
}
